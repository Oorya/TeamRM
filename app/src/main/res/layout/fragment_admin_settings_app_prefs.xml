<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:nContext="com.teamrm.teamrm.Fragment.AdminSettingsAppPrefs"
    android:orientation="vertical"
    android:background="@color/app_bg">

    <Button
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginTop="50dp"
    android:id="@+id/newCal"
    android:text="tewCal"
    android:onClick="newCal"/>

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/delit"
        android:text="delit"
        android:onClick="delit"/>

    <View
        android:layout_width="match_parent"
        android:layout_height="50dp"/>
    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="delete all tickets"
        android:id="@+id/delTickets"
        />

    <com.teamrm.teamrm.Utility.RowViewLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layoutDirection="ltr"
        android:layout_marginTop="32dp">

        <EditText
            android:id="@+id/enrollmentCodeInput"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:minWidth="60dp"
            tools:text="asfasdfasfd"
            />

        <Button
            android:id="@+id/registerAsTech"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="register as tech"
            android:layout_toEndOf="@id/enrollmentCodeLabel"
            android:layout_alignParentEnd="true"
            />
    </com.teamrm.teamrm.Utility.RowViewLayout>

</LinearLayout>
